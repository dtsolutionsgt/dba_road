SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO


ALTER VIEW [dbo].[VW_P_CANASTA_RUTA]
AS
SELECT dbo.P_CANASTA_RUTA.CODIGO_DCANASTA_CLI, dbo.P_CANASTA_RUTA.RUTA AS CODIGO_RUTA, dbo.P_RUTA.NOMBRE AS NOMBRE_RUTA,
       dbo.P_CANASTA_RUTA.PRODUCTO, dbo.P_CANASTA_RUTA.CANTIDAD, dbo.P_RUTA.SUCURSAL
FROM            dbo.P_CANASTA_RUTA INNER JOIN
                         dbo.P_RUTA ON dbo.P_CANASTA_RUTA.RUTA = dbo.P_RUTA.CODIGO
GO
