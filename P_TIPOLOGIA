CREATE TABLE [dbo].[P_TIPOLOGIA](
	[CODIGO] [nvarchar](30) NOT NULL,
	[CANALSUB] [nvarchar](10) NULL,
	[NOMBRE] [nvarchar](100) NULL,
 CONSTRAINT [PK_P_TIPOLOGIA] PRIMARY KEY CLUSTERED 
(
	[CODIGO] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO

ALTER TABLE [dbo].[P_TIPOLOGIA]  WITH CHECK ADD  CONSTRAINT [FK_P_TIPOLOGIA_P_CANALSUB] FOREIGN KEY([CANALSUB])
REFERENCES [dbo].[P_CANALSUB] ([CODIGO])
GO

ALTER TABLE [dbo].[P_TIPOLOGIA] CHECK CONSTRAINT [FK_P_TIPOLOGIA_P_CANALSUB]
GO
