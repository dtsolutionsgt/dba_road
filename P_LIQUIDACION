EXEC sp_rename 'P_LIQUIDACION.CODIGODLFC', 'CODIGODLFCR';  
EXEC sp_rename 'P_LIQUIDACION.CodigoDLFCD', 'CODIGODLFCP'; 

O
ALTER TABLE P_LIQUIDACION ADD CODIGODLFCR INT NULL
ALTER TABLE P_LIQUIDACION ADD CODIGODLFCP INT NULL

ALTER TABLE P_LIQUIDACION ADD [ID_LIQUIDACION_TIPO] INT NULL

UPDATE P_LIQUIDACION SET ID_LIQUIDACION_TIPO=1;
